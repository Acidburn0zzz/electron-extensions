{
  "manifest_version": 2,
  "name": "Test",
  "version": "1.0.0",
  "author": "sentialx",
  "description": "Test description",
  "default_locale": "en",
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "converted_from_user_script": true,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true,
      "match_about_blank": true
    }
  ]
}
